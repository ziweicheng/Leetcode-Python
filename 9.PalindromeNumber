#60ms
class Solution(object):
    def isPalindrome(self, x):
        """
        :type x: int
        :rtype: bool
        """
        if x < 0:
            return False
        if x >= 0:
            rev = 0
            ori = x
            i=0
            while x != 0:
                a = x % 10
                rev = rev*10+a
                x /= 10
                i += 1
            if rev == ori:
                return True
            else:
                return False
