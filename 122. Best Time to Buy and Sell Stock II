class Solution(object):
    def maxProfit(self, prices):
        """
        :type prices: List[int]
        :rtype: int
        """
        if not prices:
            return 0
        res = 0
        buy = prices[0]
        cache = 0
        current_max = prices[0]
        for i in range(len(prices)):
            if i == 0:
                continue
            if prices[i] > current_max:
                cache = max(prices[i] - buy, cache)
                current_max = prices[i]
            else:
                buy = prices[i]
                res += cache
                cache = 0
                current_max = prices[i]
        if cache != 0:
                res += cache
        return res
