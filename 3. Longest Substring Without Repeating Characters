class Solution(object):
    def lengthOfLongestSubstring(self, s):
        """
        :type s: str
        :rtype: int
        """
        dic = {}
        longest = 0
        compare = 0
        i = 0
        if len(s)  == 1:
            return 1 
        while i < (len(s)):
            if s[i] not in dic:
                dic[s[i]] = i
                compare += 1
                if compare > longest:
                    longest = compare
            else:
                i = dic[s[i]] + 1
                compare = 0
                dic = {}
                dic[s[i]] = i
                compare += 1
            i += 1
        return longest
