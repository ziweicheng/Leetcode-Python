class Solution(object):
    def addDigits(self, num):
        """
        :type num: int
        :rtype: int
        """
        next_val = 0
        while num > 9:   
            next_val += num % 10
            num = num // 10
        next_val += num
        if next_val > 9:
            next_val = self.addDigits(next_val)
        return next_val
        
